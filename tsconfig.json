{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@requests/*": ["src/requests/*"],
      "@static/*": ["src/static/*"],
      "@store/*": ["src/store/*"],
      "@utils/*": ["src/utils/*"],
      "@typeDefs/*": ["src/types/*"],
      "@compStyles/*": ["src/styles/components/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/(modals)/_layout.js",
    "src/components/Camera.tsx"
  ]
}
